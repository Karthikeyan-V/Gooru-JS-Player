<% if (type == 'object') {  %>
   <object width="<%= width %>px" height="<%= height %>px" data="<%= window.location%>">
<% } else if (type == 'embed') {  %>
      <embed type="text/html" src="<%= window.location%>" >
      </object>
<% } else if (type == 'iframe') { %>
      <iframe width="<%= width %>px" height="<%= height %>px" frameborder="0" src="<%= window.location%>">
<% } %>